<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.vindi.latihan">

    <!-- To access Google+ APIs: -->
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.vindi.latihan.activity.MainActivity" />
        <activity
            android:name="com.example.vindi.latihan.activity.RegisterActivity"
            android:label="Register" />
        <activity
            android:name="com.example.vindi.latihan.activity.HistoryActivity"
            android:label="History" />
        <activity android:name="com.example.vindi.latihan.activity.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.vindi.latihan.daftarActivity" />
        <activity
            android:name="com.example.vindi.latihan.activity.HotelActivity"
            android:label="Daftar Hotel" />
        <activity
            android:name="com.example.vindi.latihan.activity.DetailHotelActivity"
            android:label="Detail Hotel" />
        <activity android:name="com.example.vindi.latihan.activity.AboutActivity"></activity>
    </application>

</manifest>